    "from ffpyplayer.player import MediaPlayer\n",
    "videoPath = input(\"Enter the Video's path : \")\n",
    "player = MediaPlayer(videoPath)\n",
    "cap = cv2.VideoCapture(videoPath)\n",
    "\n",
    "while True:\n",
    "    ret, frame = cap.read()\n",
    "    cv2.imshow(\"Frame\", frame)\n",
    "    cv2.waitKey()\n",
